{
  "name": "squadbooks",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "npm run test:unit",
    "test:unit": "dotenv -e .env.test.local --override -- vitest",
    "test:unit:run": "dotenv -e .env.test.local --override -- vitest run",
    "test:unit:bootstrap": "dotenv -e .env.test.local --override -- tsx scripts/bootstrap-test-db.ts",
    "test:integration:supabase": "cross-env TEST_MODE=integration node scripts/validate-integration-db.js && dotenv -e .env.test.integration -- vitest --config vitest.config.integration.ts",
    "test:integration:supabase:run": "cross-env TEST_MODE=integration node scripts/validate-integration-db.js && dotenv -e .env.test.integration -- vitest run --config vitest.config.integration.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:coverage": "dotenv -e .env.test.local --override -- vitest run --coverage",
    "check": "npm run type-check && npm run lint && npm run format:check",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "node prisma/seed.js",
    "seed:demo": "tsx prisma/seed-demo.ts",
    "seed:preseason": "tsx scripts/seed-pre-season-budgets.ts",
    "verify:demo": "tsx prisma/verify-demo.ts",
    "prepare": "husky"
  },
